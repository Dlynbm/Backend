html
    head
        link(rel='stylesheet' href='/styles.css')
    body
        table
            tr
                th User Id:
                th Name:
                th Email:
                th Age:
            tr
            each user in users
                tr
                    td= user.individualuser.userid
                    td= user.individualuser.name
                    td= user.individualuser.email
                    td= user.age
                    td
                        a(href='/edit/' + user.userid) Edit
                    td
                        a(href='/delete/' + user.userid) Delete


    div
        a(href='/') Add another user

